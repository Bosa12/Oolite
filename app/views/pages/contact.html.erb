
<%= form_with model: @contact, url: contact_path, local: true do |form| %>
  <div>
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <div>
      <%= form.label :message %>
      <%= form.text_area :message %>
    </div>
    <div>
      <%= form.submit 'Send Message' %>
  </div>
<% end %>
