<div class="logo">
  <%= image_tag 'logo.jpg', alt: '', style: "max-width: 20vw; height: auto;" %>
</div>
<div class="container">
  <div class="row g-4">
    <% if @project.images.attached? %>
      <% @project.images.each do |image| %>
        <!-- Para telas pequenas (sm) usa toda a largura (col-12), para mÃ©dias (md) e grandes (lg) divide em 3 -->
        <div class="col-12 col-md-4 col-lg-4">
          <div class="card-proj">
            <%= image_tag url_for("https://res.cloudinary.com/dc6nbm8zp/image/upload/v1720101942/#{image.blob.filename}"), alt: @project.name, style: "width: 100%; height: 360px;" %>
            <div class="card-body">
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
<br><br><br><br><br><br>
